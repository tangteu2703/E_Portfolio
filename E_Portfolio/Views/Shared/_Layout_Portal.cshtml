						<!-- Active Toggle Script -->
						<script>
							document.addEventListener("DOMContentLoaded", function () {
							  const currentPath = window.location.pathname;
							  const navLinks = document.querySelectorAll('.navbar .nav-link');

							  navLinks.forEach(link => {
								const linkPath = new URL(link.href).pathname;

								if (linkPath === currentPath) {
								  // Thêm lớp active và border-bottom cho liên kết hiện tại
								  link.classList.add('border-bottom', 'border-2', 'border-primary');

								  // Thêm lớp text-primary cho biểu tượng bên trong
								  const icon = link.querySelector('i');
								  if (icon) {
									icon.classList.add('text-primary');
								  }

								  // Thêm thuộc tính aria-current để hỗ trợ truy cập
								  link.setAttribute('aria-current', 'page');
								} else {
								  // Xóa các lớp nếu không phải là liên kết hiện tại
								  link.classList.remove('border-bottom', 'border-2', 'border-primary');
								  const icon = link.querySelector('i');
								  if (icon) {
									icon.classList.remove('text-primary');
								  }
								  link.removeAttribute('aria-current');
								}
							  });
							});
						</script>
